<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars" xmlns:dxbt="http://schemas.devexpress.com/winfx/2008/xaml/bars/themekeys" xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core">
    
	<ControlTemplate x:Key="BarContainerControlThemeKey_BorderTemplate" TargetType="ContentControl">
        <Border UseLayoutRounding="True" Background="#FF3F3F3F" CornerRadius="3" Padding="1">
			<Border x:Name="Border" UseLayoutRounding="True" Background="#FF535353" CornerRadius="2" Padding="4" BorderBrush="{x:Null}">
				<ContentPresenter Content="{TemplateBinding Content}" />
			</Border>
		</Border>
	</ControlTemplate>
	<ControlTemplate x:Key="BarContainerControlThemeKey_BackgroundTemplate" TargetType="ContentControl">
		<Border x:Name="Background" UseLayoutRounding="True" Background="Cornsilk">
			<ContentPresenter Content="{TemplateBinding Content}" />
		</Border>
	</ControlTemplate>
</ResourceDictionary>