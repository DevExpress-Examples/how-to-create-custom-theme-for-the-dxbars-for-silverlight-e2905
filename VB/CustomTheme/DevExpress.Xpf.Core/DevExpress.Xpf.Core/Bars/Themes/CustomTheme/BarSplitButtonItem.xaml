    <ControlTemplate x:Key="BarSplitButtonItemThemeKey_ArrowTemplate" TargetType="dxb:BarSplitButtonItemArrowControl">
        <Grid Background="#00FFFFFF" x:Name="grid" Margin="2,0,3,0">
            <Border x:Name="arrow" Background="#00FFFFFF" HorizontalAlignment="Center" VerticalAlignment="Center">
                <Path Data="F1 M 1324,808L 1321,811L 1318,808L 1324,808 Z" Fill="#FF000000" Height="3" HorizontalAlignment="Center" UseLayoutRounding="True" Stretch="Fill" Margin="0,0,0,1" VerticalAlignment="Center" Width="6" />
            </Border>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="ArrowAlignment">
                    <VisualState x:Name="Right" />
                    <VisualState x:Name="Bottom">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="Margin" Storyboard.TargetName="grid" dxcn:ValueSetter.Thickness="4" />
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </Grid>
    </ControlTemplate>
    <ControlTemplate x:Key="BarSplitButtonItemThemeKey_ArrowTemplateInMainMenu" TargetType="dxb:BarSplitButtonItemArrowControl">
        <Grid Background="#00FFFFFF" x:Name="grid" Margin="2,0,3,0">
            <Border x:Name="arrow" Background="#00FFFFFF" HorizontalAlignment="Center" VerticalAlignment="Center">
                <Path x:Name="path" Data="F1M1324,808L1321,811 1318,808 1324,808z" Fill="{TemplateBinding Foreground}" Height="3" HorizontalAlignment="Center" Margin="0,0,0,1" UseLayoutRounding="True" Stretch="Fill" VerticalAlignment="Center" Width="6" />
            </Border>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="ArrowAlignment">
                    <VisualState x:Name="Right" />
                    <VisualState x:Name="Bottom">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="Margin" Storyboard.TargetName="grid" dxcn:ValueSetter.Thickness="4,4,4,4" />
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </Grid>
    </ControlTemplate>
    <ControlTemplate x:Key="BarSplitButtonItemThemeKey_ArrowTemplateInMenu" TargetType="dxb:BarSplitButtonItemArrowControl">
        <Grid Margin="3,0,4,0">
            <Path Data="F1 M 1340,804L 1344,808L 1340,812L 1340,804 Z" Fill="#FF000000" Height="8" HorizontalAlignment="Center" Stretch="Fill" VerticalAlignment="Center" Width="4" />
        </Grid>
    </ControlTemplate>
    <ControlTemplate x:Key="BarSplitButtonItemThemeKey_LargeContentBorderTemplateInRibbonPageGroup" TargetType="dxb:ItemBorderControl">
